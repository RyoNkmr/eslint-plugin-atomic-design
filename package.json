    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "ISC",
  "huskey": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
