{
  "name": "eslint-plugin-atomic-design",
  "version": "0.0.0",
  "description": "atomic design rulles",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "ryonkmr",
  "main": "lib/index.js",
